EXCHANGE=alpaca-paper
SYMBOLS=SPY,AAPL,MSFT
DECISION_INTERVAL_MS=60000
MAX_POSITION_USD=1000
MAX_ORDER_USD=250
MODELS=botA,botB
STARTING_CASH_PER_MODEL=10000

# Alpaca Paper Trading credentials (required for live paper calls)
ALPACA_API_KEY_ID=
ALPACA_API_SECRET_KEY=

# Override bases if needed
ALPACA_PAPER_BASE_URL=https://paper-api.alpaca.markets
ALPACA_DATA_BASE_URL=https://data.alpaca.markets

LOG_LEVEL=info

# LLM model configuration (per model id in MODELS)
# Example uses OpenAI; set provider/model/key for each.
MODEL_botA_PROVIDER=openai
MODEL_botA_MODEL=gpt-4o-mini
MODEL_botA_API_KEY=

MODEL_botB_PROVIDER=openai
MODEL_botB_MODEL=gpt-4o-mini
MODEL_botB_API_KEY=

# LLM limits
LLM_MAX_TOKENS=400
LLM_TEMPERATURE=0.2
LLM_MIN_CALL_INTERVAL_MS=300000


## Optional: Postgres mirror (Supabase/Neon). If set, writes are mirrored.
# DATABASE_URL=postgresql://postgres:password@host:5432/postgres
