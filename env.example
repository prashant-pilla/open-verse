EXCHANGE=alpaca-paper
SYMBOLS=AAPL,MSFT,GOOGL,AMZN,META,NVDA,TSLA,AMD,AVGO,CRM,ADBE,ORCL,INTC,CSCO,BABA,NFLX,UBER,SHOP,ABNB,SPY,QQQ,VOO,TLT,GLD,XLF,XLE,XLY,XLV,XLK,XLI,XLB,XLP,ARKK,SOXL,SMH,MU,T,JNJ,PG,KO,PEP,COST,WMT,MCD,NKE,LOW,HD,DIS,PYPL,SQ,ROKU,PLTR,COIN,RIOT,MARA,ETSY,INTU,SNOW,NET,DDOG,ZS,CRWD,PANW,OKTA,NOW,TEAM,ASML,TXN,QCOM,VRTX,REGN,MRNA,PFE,ABBV,LLY,BMY,BA,CAT,DE,GE,GM,F,RIVN,LCID,NEE,ENPH,SEDG,GEHC,TMO,DHR,FDX,UPS,KLAC,LRCX,ADSK,ANET,WDAY,ZI
DECISION_INTERVAL_MS=60000
MAX_POSITION_USD=1000
MAX_ORDER_USD=250
MODELS=botA,botB
STARTING_CASH_PER_MODEL=10000

# Alpaca Paper Trading credentials (required for live paper calls)
ALPACA_API_KEY_ID=
ALPACA_API_SECRET_KEY=

# Override bases if needed
ALPACA_PAPER_BASE_URL=https://paper-api.alpaca.markets
ALPACA_DATA_BASE_URL=https://data.alpaca.markets

LOG_LEVEL=info

# LLM model configuration (per model id in MODELS)
# Example uses OpenAI; set provider/model/key for each.
MODEL_botA_PROVIDER=openai
MODEL_botA_MODEL=gpt-4o-mini
MODEL_botA_API_KEY=

MODEL_botB_PROVIDER=openai
MODEL_botB_MODEL=gpt-4o-mini
MODEL_botB_API_KEY=

# LLM limits
LLM_MAX_TOKENS=400
LLM_TEMPERATURE=0.2
LLM_MIN_CALL_INTERVAL_MS=300000

# Optional tuning
PRICE_CONCURRENCY=8
ENABLE_MODEL_MEMORY=true
CONTEXT_MAX_ITEMS=50
SUMMARY_MAX_TOKENS=256
## Optional: Postgres mirror (Supabase/Neon). If set, writes are mirrored.
# DATABASE_URL=postgresql://postgres:password@host:5432/postgres
